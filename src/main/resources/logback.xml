<configuration>

    <appender name="STDOUT" class="ch.qos.logback.core.ConsoleAppender">
        <encoder>
            <pattern>
                %-7level %logger{20}\(%line\) - %msg%n
                <!--%d{yyyy-MM-dd HH:mm:ss} %-5level %logger{20}\(%line\) - %msg%n-->
            </pattern>
        </encoder>
    </appender>


    <root level="trace">
    <!--<root level="info">-->
        <appender-ref ref="STDOUT"/>
    </root>

</configuration>